/* pages/orderdetails/orderdetails.wxss */

page {
  width: 100%;
}

.tips {
  width: 700rpx;
  padding: 0 25rpx;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  line-height: 70rpx;
  color: #f04140;
  background: #ffe0e0;
}

.page_top_line {
  width: 100%;
  border-top: #e6e6e6 solid 1rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5;
}

.picking {
  color: #9d9d9d;
  padding: 22rpx 20rpx 25rpx 75rpx;
  background: #fff;
  position: relative;
}

.top_line {
  position: relative;
  border-top: #e6e6e6 solid 1rpx;
  top: -22rpx;
  left: -50rpx;
  z-index: 0;
  width: 730rpx;
}

.picking text {
  display: block;
  color: #333;
  width: 585rpx;
}

.picking text.picking_msg {
  font: 26rpx/34rpx 'Microsoft Yahei';
  color: #3aa11a;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.picking_relative_box {
  position: relative;
}

.consignee {
  border-bottom: #e6e6e6 solid 1rpx;
  min-height: 90rpx;
}

.picking .consignee_msg {
  color: #333;
}

.consignee_msg view {
  display: inline-block;
  overflow: hidden;
  max-width: 185rpx;
  text-overflow: ellipsis;
  height: 38rpx;
  white-space: nowrap;
}

.consignee_msg .consignee_text {
  margin-right: 10rpx;
}

.picking .consignee_address {
  font: 26rpx/38rpx 'Microsoft Yahei';
  color: #5e5e5e;
}

.picking .data_msg {
  font: 26rpx/34rpx 'Microsoft Yahei';
  color: #999;
  padding-top: 4rpx;
}

.picking .icon_arrowdown {
  position: absolute;
  width: 24rpx;
  height: 24rpx;
  right: 22rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}

.picking .icon_cart, .picking .icon_address {
  position: absolute;
  width: 34rpx;
  height: 34rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  left: -50rpx;
}

.picking_status {
  padding: 22rpx 20rpx 25rpx;
  background: #fff;
  margin-top: 20rpx;
  border-top: #e6e6e6 solid 1rpx;
  border-bottom: #e6e6e6 solid 1rpx;
  font: 26rpx/40rpx 'Microsoft Yahei';
}

.order_box {
  margin-top: 20rpx;
  padding: 22rpx 20rpx 25rpx;
  background: #fff;
  border: #e6e6e6 solid 1rpx;
  font: 26rpx/40rpx 'Microsoft Yahei';
}

.order_box .list {
  width: 100%;
  overflow: hidden;
}

.order_box text {
  float: left;
}

.wid140 {
  width: 140rpx;
}

.address {
  width: 560rpx;
  display: block;
}

.order_goods_box_list {
  position: relative;
  margin-top: 20rpx;
  border-top: #e6e6e6 solid 1rpx;
}

.order_goods_box_line {
  width: 100%;
  height: 0;
  border-bottom: #e6e6e6 solid 1rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}

.goods_list {
  background: #fff;
  padding: 22rpx 20rpx;
  overflow: hidden;
  position: relative;
}

.bottom_line {
  width: 610rpx;
  border-bottom: #e6e6e6 solid 1rpx;
  position: absolute;
  bottom: 0;
  right: 0;
}

.goods_list image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  float: left;
}

.goods_list .goods_box {
  width: 582rpx;
  color: #5e5e5e;
  margin-top: 5rpx;
  float: left;
}

.goods_list text {
  display: block;
}

.goods_list .freeGift {
  color: #e3101e;
  width: 62rpx;
  display: inline-block;
  margin-right: 20rpx;
  float: left;
  text-align: center;
  border: #e3101e solid 1rpx;
  border-radius: 5rpx;
  font: 26rpx/32rpx 'Microsoft Yahei';
  margin-top: 1px;
}

.goods_list .goods_name_box {
  width: 582rpx;
  overflow: hidden;
}

.goods_list .goods_name {
  font: 26rpx/44rpx 'Microsoft Yahei';
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.goods_list .goods_price {
  color: #ef3030;
  float: left;
  display: block;
  margin-right: 20rpx;
}

.fh {
  display: inline-block;
}

.goods_list .goods_num {
  color: #333;
  font: 26rpx/36rpx 'Arial';
  float: left;
}

.goods_price_box {
  width: 100%;
  overflow: hidden;
  font: 26rpx/36rpx 'Arial';
  padding-top: 14rpx;
}

.total_commodity {
  margin-top: 20rpx;
  padding: 20rpx 20rpx 10rpx;
  background: #fff;
  overflow: hidden;
  font: 26rpx/40rpx 'Arial';
  border-top: #e6e6e6 solid 1rpx;
}

.price_box {
  border-bottom: #ddd solid 1rpx;
  width: 710rpx;
  padding-bottom: 15rpx;
  float: left;
}

.actual_payment {
  height: 80rpx;
  font: 26rpx/80rpx 'Arial';
  padding-top: 10rpx;
}

.actual_payment .price {
  font-weight: bold;
}

.to_pay, .cancel_order {
  width: 710rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  text-align: center;
  color: #fff;
  font: 32rpx/80rpx 'Microsoft Yahei';
  margin-top: 20rpx;
  font-weight: bold;
}

.to_pay {
  background: #ff5757;
  color: #fff;
}

.cancel_order {
  background: #f8f8f8;
  color: #333;
}

.pending_payment_box {
  background: #fff;
  overflow: hidden;
  padding: 20rpx;
  border-top: #ddd solid 1rpx;
}

.pending_payment_box .to_pay {
  background: #e64340;
  border: #b83633 solid 1rpx;
}

.line-through {
  text-decoration: line-through;
}

/* 查看发票 */

.invoice-box {
  display: flex;
  align-items: center;
}

.invoice-btn {
  display: flex;
  flex-flow: nowrap;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  padding: 8rpx 20rpx;
  font-size: 24rpx;
  border: 1rpx solid #d7d8d9;
  border-radius: 100rpx;
  color: #5a6066;
  text-align: center;
}

.invoice-btn .add-ico {
  width: 27rpx;
  height: 27rpx;
  display: inline-block;
  margin-right: 10rpx;
}

.invoice-desc {
  width: 100%;
  font-size: 22rpx;
  line-height: 24rpx;
  color: #919599;
}

/* 查看多张发票 */

.layer {
  position: fixed;
  left: 0;
  top: 0;
  width: 750rpx;
  background: rgba(0, 0, 0, 0.60);
}

.layer-invoice {
  width: 750rpx;
  height: 60%;
  overflow-y: auto;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 2;
}

.layer-tit {
  position: relative;
  width: 690rpx;
  padding: 0 30rpx;
  line-height: 86rpx;
  border-bottom: 1rpx solid #ddd;
  font-size: 34rpx;
  color: #333;
  text-align: center;
}

.close-ico {
  position: absolute;
  right: 30rpx;
  top: 26rpx;
  width: 34rpx;
  height: 34rpx;
}

.layer-cont {
  width: 690rpx;
  padding: 20rpx 30rpx;
}

.layer-item {
  width: 690rpx;
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.layer-item>text {
  font-size: 30rpx;
  color: #5a6066;
  line-height: 30rpx;
  font-family: PingFangSC-Regular;
}

.layer-item .add-text {
  color: #d7d8d9;
}
