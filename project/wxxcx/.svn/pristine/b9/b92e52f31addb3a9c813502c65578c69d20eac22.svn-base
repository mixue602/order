/* ==== 订单确认页面 ==== */

/* ==== 公共样式 ==== */

.margin-b-20 {
  margin-bottom: 20rpx;
}

.margin-r-20 {
  margin-right: 20rpx;
}

.bg-white {
  background-color: #fff;
}

.bd-b {
  border-bottom: 1rpx solid #f1f1f1;
}

view {
  line-height: 0;
}

text {
  font: 26rpx/40rpx "PingFangSC-Regular";
}

.col-b1 {
  color: #262C32;
}

/* 姓名类颜色 */

.col-gray1 {
  color: #5e5e5e;
}

/* 地址类颜色 */

.col-red1 {
  color: #F14848;
}

/* 红色 */

.col-green {
  color: #1aad19;
}

.arrow {
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  right: 20rpx;
  width: 15rpx;
  height: 27rpx;
}
.mark {
  background-image: linear-gradient(-90deg, #FA1E8C 0%, #FC1E56 100%);
  border-radius: 6rpx;
  line-height: 22rpx;
  color: #fff;
  font-size: 22rpx;
  padding: 6rpx;
}

page {
  position: relative;
  min-height: 100%;
  background: #fff;
  font-family: PingFangSC-Regular;
}

.order-content {
  position: relative;
}

.topBottom {
  border-top: 1rpx solid #e3e3e3;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
}

.order-content {
  margin-top: 1rpx;
  position: relative;
  background: #f5f5f5;
  /* display: none; */
}

/* ==== 地址信息 ==== */
.streak {
  width: 100%;
  height: 6rpx;
  display: block;
}
.tishi {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background: #FFF7D2;
  color: #ff7f26;
  padding-left: 24rpx;
}

.addressWrap {
  background-color: #fff;
  position: relative;
  padding-left: 24rpx;
}

.addressWrap .addressCon {
  padding: 25rpx 0 20rpx 20rpx;
  border-bottom: 1rpx solid #ddd;
  
}

.addressWrap .addressCon .address-mark {
  display: inline-block;
}

.addressWrap .infor {
  padding-top: 28rpx;
  padding-bottom: 16rpx;
  padding-right: 52rpx;
}

.addressWrap .infor text {
  padding-right: 10rpx;
  font-size: 30rpx;
  color: #262C32;
  letter-spacing: 0;
  line-height: 30rpx;
}

.addressWrap .infor .phone {
  font-family: Arial;
}

.addressWrap .detailAddress {
  font-size: 26rpx;
  padding-right: 52rpx;
  display: -webkit-flex;
  display: flex;
  position: relative;
}
.addressWrap .detailAddress .arrow {
  top: 7rpx;
  transform: translateY(0);
  -webkit-transform: translateY(0);
}

.addressWrap .default {
  -webkit-flex: 0 0 56rpx;
  flex: 0 0 56rpx;
  text-align: center;
  font-size: 22rpx;
  line-height: 30rpx;
  border: 2rpx solid #F20C59;
  border-radius: 4rpx;
  color: #F20C59;
  height: 30rpx;
  margin-right: 14rpx;
  margin-top: 2rpx;
}

.addressWrap .address {
  -webkit-flex: auto;
  flex: auto;
  word-break:break-all;
  color: #262C32;
  display: block;
  line-height: 38rpx;
  font-size: 26rpx;
  vertical-align: middle;
}



/* ==== 产品信息 ==== */

.purchaseInfor {
  background-color: #fff;
  position: relative;
}

.purchaseInfor .inforItem {
  display: flex;
  position: relative;
  align-items: center;
  -webkit-align-items: center;
  box-sizing: border-box;
  border-bottom: 1rpx solid #ddd;
  padding-bottom: 20rpx;
  padding: 26rpx 52rpx 26rpx 24rpx;
}
.purchaseInfor .inforItem.no-skip {
  padding-right: 20rpx;
}

.purchaseInfor .bd-none {
  border: none;
}

.productInfo .imgInfo image {
  width: 100%;
  height: 100%;
}


/* ==== 产品信息显示新版 ==== */

.productInfo .purchase .imgInfo {
  flex: 0 0 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  background: url(http://css.gomein.net.cn/gmpro/1.0.0/minapp/img/default1.jpg) no-repeat;
  background-size: cover;
}
.productInfo .purchase .imgInfo image {
  width: 100%;
  height: 100%;
}
.productInfo .productDetail {
  flex: auto;
  margin-right: 35rpx;
}


.productInfo .productDetail text {
  color: #333;
  width: 100%;
  display: box;
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  box-orient: vertical;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  line-height: 30rpx;
  font-size: 24rpx;

}

.productInfo .purchase .other {
  flex: 1 0 140rpx;
  text-align: right;
}
.productInfo .purchase .other text {
  display: block;
  line-height: 36rpx;
}

.productInfo .purchase .other .num {
  color: #333;
}

.productInfo .purchase .moreImgs {
  width: 1;
  flex: 1;
  -webkit-flex: 1;
}

.productInfo .purchase .moreImgs image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}

.productInfo .purchase .total {
  width: 150rpx;
  text-align: center;
  font-size: 24rpx;
}

.productInfo .purchase .total text {
  color: #F14848;
  padding: 0 10rpx 0 5rpx;
}

/* ==== 返券 赠品 ==== */

.productInfo .attach {
  display: flex;
  padding-bottom: 15rpx;
}

.productInfo .attach .title {
  width: 80rpx;
}

.productInfo .attach .title text {
  position: static;
  transform: translateY(0);
}

.productInfo .attachCon {
  flex: 1;
}

.productInfo .attachCon .list {
  width: 100%;
  display: box;
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  box-orient: vertical;
  -webkit-line-clamp: 1;
  line-clamp: 1;
}

.productInfo .attachCon text {
  color: #999;
}

/* ==== 配送方式、发票信息等 */

.purchaseInfor .title {
  width: 200rpx;
  position: relative;
}

.purchaseInfor .title text {
  color: #262C32;
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
}

.purchaseInfor .detail {
  position: relative;
  flex: 1;
  text-align: right;
  min-height: 36rpx;
}

.purchaseInfor .inforItem .detail text {
  line-height: 36rpx;
  font-size: 24rpx;
  color: #262C32;
  width: 100%;
  display: box;
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  box-orient: vertical;
  -webkit-line-clamp: 1;
  line-clamp: 1;
}

/* ==== 发票信息 ==== */

.purchaseInfor.ticketInfor .inforItem .detail text {
  display: inline-block !important;
  width: auto;
  line-height: 36rpx;
}
.purchaseInfor.ticketInfor .inforItem .detail .invoice-type {
  color: #919599;
  margin-right: 5rpx;
}


/* ==== 优惠券 ==== */
.purchaseInfor.counpon-desc .title {
  width: 200rpx;
}
.purchaseInfor.counpon-desc .title .reduce-num {
  position: absolute;
  left: 100rpx;
  color: #fff;
}
.purchaseInfor.counpon-desc .inforItem .detail .fee-mark {
  color: #F14848;
}

/* ==== 备注 ==== */
.comment-box .title {
   width: 140rpx;
}
.comment-box input {
  text-align: left;
  line-height: 50rpx;
  height: 40rpx;
  width: 100%;
  box-sizing: border-box;
  font-size: 26rpx;
  padding: 0 10rpx;
  color:#262C32;
  min-height: 40rpx;
  margin-top: 5rpx;
}



/* ==== 计算金额类样式 ==== */

.calculateCmountWrap {
  background-color: #fff;
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
  padding-left: 20rpx;
}

.calculateCmountWrap .calculateCmount {
  padding: 24rpx 30rpx 24rpx 0;
  border-bottom: 1rpx solid #ddd;
}

.calculateCmountWrap .calculateCmount .item {
  display: flex;
  line-height: 36rpx;
}

.calculateCmountWrap .title {
  width: 130rpx;
  color: #262C32;
  overflow: hidden;
}

.calculateCmountWrap .title  text {
  float: left;
}

.calculateCmountWrap .title .titleImg {
  width: 30rpx;
  height: 30rpx;
  float: left;
  margin-top: 5rpx;
  margin-left: 5rpx;
}

.calculateCmountWrap .detail {
  flex: 1;
  position: relative;
  text-align: right;
 color: #F14848;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.calculateCmountWrap .detail text {
  color: #F14848;
}

.calculateCmountWrap .amountPaid {
  line-height: 95rpx;
  padding-right: 30rpx;
  text-align: right;
}

/* ==== 支付方式 ==== */

.payment {
  background-color: #fff;
  padding: 1rpx 24rpx 30rpx;
}

.payment .payBtn {
  display: block;
  height: 80rpx;
  border: 1rpx solid #0c9e0b;
  padding: 0;
  line-height: 78rpx;
  background-color: #1aad19;
  color: #fff;
  text-align: center;
  margin-top: 20rpx;
  border-radius: 6rpx;
  font-size: 32rpx;
  font-weight: 500;
}

.payment .cashOnDelivery {
  border-color: #e6e6e6;
  background-color: #f8f8f8;
  color: #333;
  text-align: center;
}
.payment .payBtn.disablePay {
  opacity: 0.6;
}

/*运费信息*/

.freight-box {
  position: fixed;
  width: 750rpx;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  left: 0;
  top: 0;
  z-index: 1000;
}

.freight {
  position: fixed;
  left: 95rpx;
  top: 10%;
  width: 560rpx;
  max-height:80%;
  overflow-y: auto;
  background: #fff;
  border-radius: 20rpx;
  z-index: 100000;
}

.freight-tit {
  width: 460rpx;
  padding: 24rpx 50rpx;
  border-bottom: 1rpx solid #d2d3d5;
  height: 40rpx;
  overflow: hidden;
}

.tit-h {
  float: left;
  font: 400 32rpx/40rpx "PingFangSC-Regular";
  color: #333;
}

.tit-info {
  float: right;
  text-align: right;
  font: 400 26rpx/40rpx "PingFangSC-Regular";
  color: #333;
}

.freight-content {
  position: relative;
  width: 460rpx;
  padding: 20rpx 50rpx 30rpx 50rpx;
  overflow: hidden;
}

.freight-content .cont-tit {
  font: 400 28rpx/40rpx "PingFangSC-Regular";
  color: #333;
}

.freight-item-info {
  width: 460rpx;
  height: 30rpx;
  padding: 15rpx 0;
  font: 400 24rpx/30rpx "PingFangSC-Regular";
  overflow: hidden;
}

.freight-item-info>view {
  float: left;
  line-height: 30rpx;
  padding-right: 10rpx;
  color: #888;
}

.freight-item-prod {
  width: 100%;
  height: 100rpx;
  overflow-x: scroll;
  overflow-y: hidden;
}

.freight-item-prod view {
  width: 100rpx;
  height: 100rpx;
  display: block;
  float: left;
}

.freight-item-prod .item-iamge {
  width: 100rpx;
  height: 100rpx;
}

.freight-item-prod .item-left {
  margin-left: 23rpx;
}

/*不能提交订单的商品*/
.prod-container {
  position:fixed;
  left:0;
  top:0;
  padding:60px 0;
  margin-left:65rpx;
  margin-right:65rpx;
  box-sizing:border-box;
}
.no-box {
  background:#fff;
  padding-top:122rpx;
  padding-bottom:89rpx;
  display:flex;
  flex-direction:column;
  box-sizing:border-box;
  border-radius:20rpx;
}
.no-tit {
 position:absolute;
  padding:30rpx 40rpx 0 40rpx;
  text-align:center;
  top:60px;
  left:0;
  line-height:46rpx;
  font-size:32rpx;
  color:#333;
  letter-spacing:-0.72rpx;
  background:#fff;
  border-top-left-radius:20rpx;
  border-top-right-radius:20rpx;
}

.no-list {
  padding: 0 30rpx;
  position: relative;
  display: flex;
  flex-direction: column;
}

.no-prod-box {
  width: 560rpx;
  padding-top: 20rpx;
  display: flex;
  flex-direction: column;
}

.no-prod {
  width: 560rpx;
  padding-top: 20rpx;
  display: flex;
  flex-direction: row;
}

.no-prod-cont {
  display: flex;
  flex-direction: column;
  width: 418rpx;
  padding-left: 20rpx;
  height: 120rpx;
}

.no-prod-name {
  width: 418rpx;
  font-size: 24rpx;
  color: #262c32;
  line-height: 34rpx;
  height: 68rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.no-prod-name-tags {
  border: 1rpx solid #ff5c5c;
  padding:0 2rpx;
  font-size: 18rpx;
  color: #ff5c5c;
  line-height: 20rpx;
  border-radius: 5rpx;
  margin-right: 5rpx;
}

.no-prod-info {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: 418rpx;
  height: 30rpx;
  padding-top: 22rpx;
  font-size: 26rpx;
}

.no-prod-num {
  color: #999;
  line-height: 30rpx;
}

.no-prod-status {
  font-size: 24rpx;
  color: #ff5c5c;
  padding-left: 20rpx;
  line-height: 30rpx;
}

.no-prod-img {
  width: 120rpx;
  height: 120rpx;
  border: 1rpx solid #ddd;
}

.no-prod-img>image {
  width: 120rpx;
  height: 120rpx;
}

.tao-tit {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 560rpx;
  height: 32rpx;
  padding: 20rpx 0;
}

.tao-tit-tags {
  width: 56rpx;
  height: 22rpx;
  font-size: 22rpx;
  line-height: 22rpx;
  text-align: center;
  color: #fff;
  background: #ff5c5c;
  border-radius: 4rpx;
  padding: 5rpx;
}

.tao-tit-info {
  padding-left: 11rpx;
  width: 493rpx;
  height: 32rpx;
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #262c32;
  line-height: 32rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.tao-tips {
  position: relative;
  width: 160rpx;
  height: 32rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 32rpx;
  background: #919599;
  border-radius: 3rpx;
}

.no-prod-span {
  position: relative;
  width: 560rpx;
  height: 1rpx;
  background: #ddd;
  margin-top: 20rpx;
}

.no-prod-btn {
  width:100%;
  position:absolute;
  bottom:60px;
  height:89rpx;
  border-top:1rpx solid #ddd;
  display:flex;
  flex-direction:row;
  align-items:center;
  font-size:32rpx;
  line-height:89rpx;
  color:#00050b;
  background:#fff;
  border-bottom-left-radius:20rpx;
  border-bottom-right-radius:20rpx;
}

.return-btn {
  width: 50%;
  line-height: 89rpx;
  border-right: 2rpx solid #ddd;
  text-align: center;
}

.del-no-btn {
  width: 50%;
  line-height: 89rpx;
  text-align: center;
}
