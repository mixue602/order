page{ 
  background: #fff;
  position: relative;
  height: 100%;
 }
 .mask {
    position: absolute;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.2);
    left: 0;
    top: 0;
    z-index: 1000000;
 }
.search{ width: 100%; padding-top: 15rpx; position: fixed;  background: #fff;left: 0;top:0;z-index: 999;}

.search-container{ width: 100%; position: relative; }

.search-box{
  padding: 20rpx;
  height: 58rpx;
  margin:0 auto 10rpx;
  font-size: 24rpx;
  -webkit-display: flex;
  display: flex;

  background: #fff;
}
.search-box .input-wrapper {
    -webkit-flex: 1;
    flex: 1;
    position: relative;
}
.search-box .search-input{
  width: 100%;
  height: 58rpx;
  font: 500 28rpx/60rpx "Microsoft YaHei";
  background: #f3f3f3;
  border-radius: 10rpx;
  padding-left: 80rpx;
  padding-right: 50rpx;
  color: #333;
  box-sizing: border-box;
}

.search-box  .search_icon{
  position: absolute;
  top:15rpx;
  left: 20rpx;
  z-index: 999;
}
.search_cancel {
    position:absolute;
    top:0rpx;
    right:0rpx;
    display:inline-block;
    width:32rpx;
    height:32rpx;
    z-index:99;
    padding:13rpx 9rpx;

}
.search-box button{
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  padding: 0 20rpx;
  background: #e3101e;
  color: #fff;
  font-size: 28rpx;
  margin-left: 10rpx;
}
.search-box .default{
   background: #f8f8f8;
   color: #333;
}
.search-box .warn{
   background: #e3101e;
   color: #fff;
}
.search-box .button-hover{
  background: red;
  color: #fff;
}

.search-tips{
  left: 0rpx;
  top: 96rpx;
  background: #fff;
  color: #666;
  width: 100%;
  padding: 0 25rpx;
  position: absolute;
  z-index: 999;
}


.search-tips >view{
  width: 100%;
  border-bottom: 1rpx solid #eee;
  line-height: 3em;
  font-size: 30rpx;
  font-family: "Microsoft YaHei";
}

.search-cont{
  border-top:2rpx solid #e6e6e6;
  width: 100%;
}
.hot-search{
  padding: 0 10rpx 20rpx 10rpx;
}
.search-history{
   padding: 10rpx 10rpx 30rpx 10rpx;
   position: relative;
}
.search-history .historyItem{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  max-width: 40%;
}
.hot-search .search-tit,.search-history .search-tit{
  height: 60rpx;
  font: normal 28rpx/60rpx "Microsoft YaHei";
  padding-left:20rpx;
  color: #5e5e5e;
}
.history-box{
  overflow: hidden;
}

.hot-box .hotItem,.history-box .historyItem {
  display: inline-block;
  height: 50rpx;
  border: none;
  padding: 0 20rpx;
  text-align: center;
  margin-left: 20rpx;
  font: 500 28rpx/50rpx "Microsoft YaHei";
  border-radius: 10rpx;
  margin-top: 20rpx;
  color: #333;
  background: #efeff4;
}
.hot-box .hotItem:hover,.history-box .historyItem:hover{
  background: #e6e6e6;
}

.history-no{
  width: 80rpx;
  height: 40rpx;
  font: 500 24rpx/40rpx "Microsoft YaHei";
  color: #e3101e;
  position: absolute;
  right: 15rpx;
  top: 20rpx;
}

.hide{
  display: none;
}
.clear{
  color:#fff;
  background: red;
}
.search-lists-wrapper {
    width: 100%;
    position: fixed;
    box-sizing: border-box;
    top: 111rpx;
    left: 0;
}

.search-tab-box{ display: flex; width:100%; background: white; border-top: 1px solid #e6e6e6; border-bottom: 1px solid #e6e6e6; font-size: 24rpx;
}
.search-tab-box .search-tab{ height: 36rpx; line-height: 36rpx; padding: 15rpx 0; flex: 1; text-align: center; position: relative; font-size: 0rpx; }
.search-tab-box .search-tab text {font-size: 30rpx;}
.search-tab-box .filterType image{ width: 28rpx; height: 28rpx; vertical-align: top; margin-top: 4rpx; margin-left: 10rpx; }
.search-tab-box .active{ color: red;}

.search-lists-results {
  padding-top: 185rpx;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.changep{padding-top:248rpx;}
.search-lists-con{ font-family: 'Microsoft Yahei';}
.search-list{ width: 100%; margin: 0 auto; height:100%;}
.search-list .listItem{ display: flex; margin: 15rpx 0; padding: 10rpx 20rpx 0; border: 1px solid #fff; cursor: pointer; }
.search-list .listItem-hover{ background: #f3f3f3; }
.search-list .listItem .list-img{ width: 250rpx; height: 250rpx; /*background: url(http://css.gomein.net.cn/gmpro/1.0.0/minapp/img/default1.jpg) no-repeat; background-size: 95%; */}
.search-list .listItem .list-img image{ width: 98%; height: 98%; }
.search-list .listItem .list-txt{ flex: 1;border-bottom:1rpx solid #e6e6e6;
}
.search-list .listItem .list-txt .txt-tit{display: flex; font-size: 28rpx; margin-bottom: 15rpx; line-height: 1.5em;}
.search-list .listItem .list-txt .txt-tit .txt-mark{ width: 40px; }
.search-list .listItem .list-txt .txt-tit .txt-name{ flex: 1; display: -webkit-box; height: 3em;-webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden; text-overflow: ellipsis; word-break:break-all; color: #333;}
.search-list .listItem .list-txt .txt-tit .txt-tag{ color: #e3101e; font-size: 22rpx; border-radius: 4rpx; border:1rpx #e3101e solid; height: 1.5em; line-height: 1.5em; padding: 0 8rpx; margin-right: 15rpx; }
.search-list .listItem .list-txt .priceType-wrapper{  width:100%;height: 30rpx;overflow:hidden;line-height: 30rpx;font-size: 0;}
.search-list .listItem .list-txt .return-money{
    background: #ffcbcb;color: #ff5c5c;
    font-size:0;
    margin-top:3rpx;
    vertical-align:top;
    display:inline-block;
    border-radius:1rpx;
    height:30rpx;
    line-height:30rpx;
}
.return-money .txt {
    vertical-align:top;
    display:inline-block;
    color:#e3101e;
    font-size:20rpx;
    line-height:30rpx;
    padding:0 8rpx;
}
.search-list .listItem .list-txt .txt-pri{
    display: inline-block;font-size: 30rpx; color: #e3101e; margin-right: 30rpx;vertical-align: top;
}
.search-list .listItem .list-txt .priceType{font-size: 0;margin-top:3rpx;vertical-align: top;display: inline-block;border: 1px solid #f00;border-radius: 1rpx;height: 22rpx;line-height: 22rpx;}
.search-list .listItem .list-txt .priceType .image-box{background: #ef3030;display: inline-block;width:26rpx; height: 22rpx;border-right: 1px solid #f00;}
.search-list .listItem .list-txt .priceType image{width: 20rpx;height: 20rpx;vertical-align: top;margin-top: 1rpx; margin-left: 3rpx;
}
.search-list .listItem .list-txt .priceType .txt {
  vertical-align: top;
  display: inline-block;
  color: #e3101e;
  font-size: 18rpx;
  line-height: 22rpx;
  padding: 0 8rpx; 
}
.search-list .listItem .list-txt .txt-evaluateCount{
    width:100%; font-size: 24rpx; color: #a2a4a6;
}

.show{display:inline;}
.hidden{display:none;}
.span{display: inline;}


.pri_updown{ width: 18rpx; height: 24rpx;  display: inline-block;vertical-align: top; margin-top: 6rpx; margin-left: 15rpx;}
.coupbg{box-sizing:border-box;display: inline-block; width:100%; background:#fff7d2; border-bottom: 1px solid #e6e6e6;  font-size: 22rpx; padding: 15rpx 0; font: 400 22rpx/30rpx "Microsoft YaHei"; text-align:center;color:#ff8000}

.loadMore{ text-align: center; font-size: 24rpx; }