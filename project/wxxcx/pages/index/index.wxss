.container {
  display: flex;
  flex-direction: column;
  width: 750rpx;
  font-family: PingFangSC-Regular;
}

/* 顶部轮播 */

.banner-box, .banner {
  width: 750rpx;
  height: 360rpx;
  position: relative;
}

.QR-btn {
  position: absolute;
  top: 15rpx;
  left: 25rpx;
  width: 54rpx;
  color: #fff;
  font-size: 18rpx;
  text-align: center;
  line-height: 25rpx;
}

.QR-btn>image {
  width: 40rpx;
  height: 40rpx;
}

/*搜索  */

.search-box {
  top: 13rpx;
  left: 104rpx;
  text-align: center;
  width: 542rpx;
  height: 60rpx;
  position: absolute;
}

.search-btn {
  width: 542rpx;
  height: 60rpx;
  font: 400 28rpx/65rpx 'Microsoft YaHei';
  background: #fff;
  text-align: left;
  color: #919599;
  border-radius: 40rpx;
  display: flex;
  align-items: center;
}

.search-btn image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
  margin-left: 16rpx;
}

/* 活动按钮 */

.active-btn {
  position: absolute;
  top: 13rpx;
  right: 22rpx;
  width: 60rpx;
  height: 60rpx;
  /* background: #582ed0; */
  border-radius: 30rpx;
}

.active-btn>image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
}

/*用来包裹所有的小圆点  */

.dots {
  height: 6rpx;
  display: flex;
  flex-direction: row;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 18rpx;
}

.daily-dots {
  width: 710rpx;
  position: relative;
  display: flex;
  justify-content: center;
  background: #fff;
  padding: 8rpx 0 20rpx 0;
}

/*未选中时的小圆点样式 */

.dot {
  width: 9rpx;
  height: 6rpx;
  margin-right: 6rpx;
  background: rgba(255, 255, 255, 0.50);
  border: 1px solid rgba(0, 5, 10, 0.10);
  border-radius: 3.5px;
}

.daily-dot {
  width: 9rpx;
  height: 6rpx;
  margin-right: 6rpx;
  background: rgba(0, 5, 10, 0.10);
  border-radius: 3.5rpx;
}

/*选中以后的小圆点样式  */

.active {
  width: 32rpx;
  height: 6rpx;
  background-color: #fff;
}

.daily-active {
  width: 32rpx;
  height: 6rpx;
  background: #f20c59;
  border-radius: 3.5rpx;
}

/*快捷入口*/

.enter-list {
  position: relative;
  width: 750rpx;
  padding: 20rpx 0 0;
  display: flex;
  flex-direction: column;
}

.enter-box {
  padding: 10rpx;
  width: 690rpx;
  margin-left: 20rpx;
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.enters-list {
  width: 690rpx;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-around;
  align-items: center;
}

.enters {
  display: inline-block;
  margin: 10rpx auto;
}

.index-tit {
  flex: 1;
  text-align: center;
}

.enter-desc {
  overflow: hidden;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #5a6066;
  white-space: nowrap;
  text-align: center;
  width: 96rpx;
  margin: 0 auto;
}

.index-tit image {
  width: 86rpx;
  height: 86rpx;
  border-radius: 86rpx;
}

.n_dots {
  width: 100%;
  height: 6rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  transform: translateX(-50%);
}

.n_dots .dot {
  width: 9rpx;
  height: 6rpx;
  margin-right: 6rpx;
  background: rgba(0, 5, 10, 0.10);
  border: 1px solid rgba(0, 5, 10, 0.10);
  border-radius: 3.5rpx;
}

.n_dots .active {
  background: #f20c59;
}

/* 国美头条 */

.headline-box {
  width: 710rpx;
  margin-left: 20rpx;
  display: flex;
  align-items: center;
}

.headline-tit {
  width: 72rpx;
  height: 36rpx;
}

.headline-list {
  height: 80rpx;
  padding-left: 10rpx;
  overflow: hidden;
}

.headline-tags {
  display: flex;
  align-items: center;
}

.headline-tag {
  padding: 5rpx 10rpx;
  border: 1rpx solid #f20c59;
  color: #f20c59;
  border-radius: 5rpx;
  font-size: 12rpx;
  line-height: 40rpx;
}

/* 国美秒杀 */

.ms-box {
  position: relative;
  width: 710rpx;
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  background: #fff;
}

.ms-container {
  width: 710rpx;
}

.ms-tit {
  width: 710rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  line-height: 45rpx;
  color: #262c32;
}

.rush-tit {
  width: 680rpx;
  padding: 18rpx 15rpx;
}

.tit-left {
  display: flex;
  align-items: center;
}

.tit-name {
  font-family: PingFang-SC-Medium;
  font-size: 32rpx;
  max-width: 128rpx;
  line-height: 45rpx;
  height: 45rpx;
  overflow: hidden;
}

.tit-desc {
  font-size: 24rpx;
  padding: 0 12rpx;
  line-height: 45rpx;
  height: 45rpx;
  max-width: 280rpx;
  overflow: hidden;
  color: #919599;
}

.tit-desc-ms {
  font-size: 28rpx;
  padding: 0 12rpx;
  line-height: 45rpx;
  height: 45rpx;
  max-width: 280rpx;
  overflow: hidden;
}

.tit-time {
  padding-left: 10rpx;
  display: flex;
  align-items: center;
  text-align: center;
}

.time {
  width: 30rpx;
  height: 28rpx;
  font-size: 20rpx;
  line-height: 28rpx;
  background: #536079;
  border-radius: 4rpx;
  color: #fff;
}

.time-span {
  font-size: 20rpx;
  line-height: 28rpx;
  color: #33383e;
  padding: 0 8rpx;
}

.tit-right {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  line-height: 45rpx;
  color: #919599;
}

.go-ico {
  width: 16rpx;
  height: 30rpx;
  margin-left: 8rpx;
}

.ms-cont {
  width: 670rpx;
  box-sizing: border-box;
}

.ms-cont-many {
  width: 710rpx;
}

.ms-list {
  display: flex;
  white-space: nowrap;
  align-items: center;
  padding-bottom: 15rpx;
}

.ms-item {
  display: inline-block;
  width: 160rpx;
  margin-right: 20rpx;
}

.ms-image {
  width: 160rpx;
  height: 160rpx;
}

.ms-price {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #f20c59;
  text-align: center;
}

.ms-desc {
  font-size: 20rpx;
  line-height: 28rpx;
  color: #919599;
  text-align: center;
}

.daily-item {
  display: inline-block;
  width: 174rpx;
  height: 280rpx;
  margin-right: 20rpx;
}

.daily-item.m-15 {
  margin-left: 15rpx;
}

.daily-image {
  width: 174rpx;
  height: 174rpx;
  display: block;
 
}

.daily-tags {
  padding-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 30rpx;
}

.prod-daily-tags {
  display: flex;
  align-items: center;
}

.daily-tag {
  border: 1rpx solid #cacccf;
  color: #7a7f85;
  font-size: 20rpx;
  line-height: 30rpx;
  border-radius: 14rpx;
  padding: 0 7rpx;
  
}

.prod-daily-tag {
  border: 1rpx solid #f20c59;
  color: #f20c59;
  font-size: 20rpx;
  line-height: 26rpx;
  border-radius: 3rpx;
  padding: 0 5rpx;
  box-sizing: border-box;
  overflow: hidden;
  
}

.daily-tag-right {
  margin-left: 6rpx;
}

.prod-daily-tag-right {
  margin-left: 10rpx;
}

.daily-price {
  width: 100%;
  text-align: center;
  color: #f20c59;
  margin-top: 10rpx;
}

/* 楼层图片 */

.floor {
  width: 710rpx;
  margin-left: 20rpx;
  overflow: hidden;
  background: #fff;
  /* color:#f20c59; */
}

.mg-top {
  margin-top: 20rpx;
}
.floor .carouse-tit {
  display: block;
}
.fl-list-box {
  width: 710rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.fl-list {
  width: 710rpx;
  height: auto;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.fl-item {
  flex: 1;
  text-align: center;
}

.fl-img {
  width: 100%;
  display: block;
  overflow: hidden;
}

/* 大小图 */

.fl-big-small-box {
  width: 710rpx;
  display: flex;
  align-items: center;
}

.fl-list-left {
  display: flex;
  flex-direction: column;
  flex: 1;
}

.fl-list-right {
  display: flex;
  flex-direction: column;
  flex: 1;
}

.fl-list-right-top {
  display: flex;
  /* flex-direction: column; */
  flex: 1;
}

.fl-list-right-bottom {
  display: flex;
  flex-direction: row;
  flex: 1;
}

.fl-list-right-bottom.three {
  flex-direction: column;
}

.fl-list-right .fl-item {
  display: flex;
  flex-direction: column;
  flex: 1;
}

/* 无边线 */

.border_0 {
  border: none;
}

/* 全边线 */

.border_1 {
  border-left: 1rpx solid #f2f2f2;
}

/* 内边线 */

.border_2 {
  border-right: 1rpx solid #f2f2f2;
}

/* 上边线 */

.border_3 {
  border-top: 1rpx solid #f2f2f2;
}

/* 下边线 */

.border_4 {
  border-bottom: 1rpx solid #f2f2f2;
}

/* 商品大图轮播 */

.daily-box, .daily {
  width: 710rpx;
  height: 250rpx;
  position: relative;
  background: #fff;
}

/* 商品大图轮播 */

.goods-box {
  width: 710rpx;
  height: 300rpx;
  position: relative;
}

.prod-box {
  width: 710rpx;
  display: flex;
  /* align-items: center; */
}

.prod-img {
  width: 210rpx;
  height: 210rpx;
  margin-top: 20rpx;
  margin-left: 15rpx;
}

.prod-item {
  width: 410rpx;
  padding-left: 20rpx;
  padding-top: 20rpx;
}

.prod-desc {
  width: 410rpx;
  height: 72rpx;
  font-size: 26rpx;
  line-height: 36rpx;
  overflow: hidden;
  padding-top: 10rpx;
  /* color: #00050a; */
}

.prod-price {
  margin-top: 25rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #f20c59;
}

/*广告图  */

.ad-list {
  width: 750rpx;
  position: fixed;
  top: 0;
  left: 0;
}

.ad-list-bg {
  width: 750rpx;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  top: 0;
  left: 0;
  box-sizing: border-box;
}

.del {
  width: 65rpx;
  height: 65rpx;
  margin-top: 175rpx;
  margin-left: 625rpx;
  position: relative;
}

.cancel {
  width: 65rpx;
  height: 65rpx;
}

.ad-box {
  position: absolute;
  top: 240rpx;
  left: 60rpx;
  width: 630rpx;
  height: 660rpx;
  z-index: 99;
}

.ad-box >view {
  width: 100%;
  height: 100%;
}

.ad-box image {
  width: 100%;
  height: 100%;
}

/* goods */

.goods {
  width: 750rpx;
  overflow: hidden;
}

.goods.mg-top {
  margin-top: 20rpx;
}

.goods-list {
  width: 720rpx;
  padding: 0 10rpx 0 20rpx;
  display: flex;
  flex-flow: row wrap;
}

.goods-item {
  margin-bottom: 10rpx;
  width: 350rpx;
  margin-right: 10rpx;
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.goods-img {
  width: 350rpx;
  height: 350rpx;
  overflow: hidden;
  display: block;
}

.goods-desc {
  width: 310rpx;
  height: 72rpx;
  padding: 0 20rpx;
  margin-top: 20rpx;
  font-size: 26rpx;
  line-height: 36rpx;
  color: #00050a;
  overflow: hidden;
}

.goods-price {
  width: 310rpx;
  padding: 20rpx 20rpx 30rpx;
  font-size: 32rpx;
  color: #f20c59;
  text-align: justify;
  line-height: 32rpx;
}

.no-text {
  width: 750rpx;
  padding: 30rpx 0;
  text-align: center;
  font-size: 26rpx;
  color: #5a6066;
  line-height: 36rpx;
}

.carouse-list {
  display: flex;
  white-space: nowrap;
  align-items: center;
  padding-bottom: 20rpx;
  /* padding-left: 20rpx; */
  padding: 20rpx 0;
}

.carouse-item {
  width: 174rpx;
  margin-right: 8rpx;
}
