page {
    background-color: #f3f3f3;
}

.dou-head {
    width: 100%;
    height: 200rpx;
    background: url("http://css.gomein.net.cn/gmpro/1.0.0/minapp/img/dou-bg.png") no-repeat;
    background-size: 100% 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-family: PingFangSC-Regular;
    font-size: 34rpx;
    color: #fff;
    letter-spacing: 0;
}

.dou-head image {
    width: 48rpx;
    height: 46rpx;
    margin-right: 10rpx;
}

.dou-body {
    background-color: #fff;
    width: 694rpx;
    padding: 0 30rpx;
}

.dou-head-con {
    font-family: PingFangSC-Regular;
    font-size: 60rpx;
    color: #fff;
    letter-spacing: 0;
}

.dou-title {
    width: 690rpx;
    height: 100rpx;
    line-height: 100rpx;
    margin: 0 auto;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #999;
    letter-spacing: 0;
    box-sizing: border-box;
    border-bottom: solid 1rpx #d8d8d8;
}

.dou-details {
    height: 120rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.detail-left {
    display: flex;
    flex-direction: column;
}

.detail-shop {
    width: 550rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #666;
    letter-spacing: 0;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.detail-time {
    font-family: PingFangSC-Regular;
    font-size: 22rpx;
    color: #666;
    letter-spacing: 0;
    text-align: left;
}

.detail-num {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    letter-spacing: 0;
    text-align: left;
}

.num-red {
    color: #f20c59;
}

.num-green {
    color: #15a100;
}

.con-end {
    margin: 0 auto;
    margin-top: 23rpx;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #a5a5a5;
    letter-spacing: 0;
}

.gotop {
    position: fixed;
    right: 43rpx;
    bottom: 22rpx;
    width: 100rpx;
    height: 100rpx;
    z-index: 500;
}

.gotop .gotop-img {
    width: 100%;
    height: 100%;
}

.nodou {
    margin: 0 auto;
    width: 200rpx;
    height: 200rpx;
    padding-top: 180rpx;
}

.nodou-img {
    width: 100%;
    height: 100%;
}

.nodou-content {
    margin-top: 8rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #999999;
    letter-spacing: 0;
    text-align: center;
}