/* pages/profile/profile.wxss */

page {
    font-family: Helvetica Neue, Helvetica, Arial, sans-serif, 'Microsoft Yahei', 'PingFangSC-Regular';
}

.ticket_wrap {
    width: 680rpx;
    height: 390rpx;
    margin: 30rpx auto 0;
    background: #FFFFFF;
    box-shadow: 0 5rpx 13px 0 rgba(0, 0, 0, 0.09);
    border-radius: 10rpx;
    position: relative;
}

.ticket_title_box {
    padding: 20rpx;
    height: 48rpx;
    overflow: hidden;
}

.ticket_title {
    font-size: 32rpx;
    color: #000000;
    line-height: 48rpx;
    float: left;
    max-width: 565rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.ticket_title_down {
    font-size: 32rpx;
    color: #000000;
    line-height: 48rpx;
    float: left;
    max-width: 470rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.get_button {
    width: 146rpx;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 26rpx;
    color: #FFFFFF;
    text-align: center;
    background-image: linear-gradient(-222deg, #FF336C 0%, #FF0044 100%);
    border-radius: 100rpx;
    float: right;
}

.line {
    width: 634rpx;
    height: 0;
    border-bottom: 2rpx dashed #D7D8D9;
    margin-left: 24rpx;
}

.ticket_image_box {
    width: 640rpx;
    height: 256rpx;
    margin: 22rpx 20rpx;
    border-radius: 8rpx;
    position: relative;
    background: #F3F5F7;
    vertical-align: top;
}

.ticket_image_box view {
    font-size: 32rpx;
    color: #919599;
    position: absolute;
    top: 100rpx;
    left: 160rpx;
}

.ticket_image_box image {
    width: 304rpx;
    height: 148rpx;
    margin: 54rpx 0 0 168rpx;
}

.semicircle {
    width: 24rpx;
    height: 24rpx;
    border-radius: 50%;
    background: #F3F5F7;
    position: absolute;
    top: 76rpx;
}

.semicircle_left {
    left: -12rpx;
}

.semicircle_right {
    right: -12rpx;
}

.icon {
    width: 140rpx;
    height: 140rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.icon image {
    width: 100%;
    height: 100%;
}

.ticket_hd {
    margin: 77rpx 0 48rpx 222rpx;
    width: 298rpx;
    height: 34rpx;
    text-align: center;
    border: #262C32 solid 4rpx;
    border-top-width: 0;
    border-bottom-width: 0;
    line-height: 34rpx;
    font-size: 34rpx;
    color: #26282D;
}

.no_more_box {
    width: 550rpx;
    margin: 194rpx 0 0 100rpx;
}

.no_more_box text {
    width: 100%;
    display: inline-block;
    font-size: 32rpx;
    line-height: 50rpx;
    color: #919599;
    text-align: center;
}

.go_index {
    width: 180rpx;
    height: 74rpx;
    font-size: 28rpx;
    line-height: 74rpx;
    text-align: center;
    background: #FF5C5C;
    border-radius: 86rpx;
    margin: 22rpx 0 0 186rpx;
    color: #FFFFFF;
}

.nomore {
    color: #5A6066;
    font: 26rpx/30rpx 'Microsoft Yahei';
    padding: 30rpx 0;
    width: 100%;
    text-align: center;
}

.fixed_box {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    display: flex;
    justify-content: center;
    align-items: center;
}

.fixed_bg {
    width: 100%;
    height: 100%;
    opacity: 0.7;
    background: #000000;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 0;
}

.fixed_ticket_box {
    position: relative;
    z-index: 1;
    width: 670rpx;
    background: #FFFFFF;
    box-shadow: 0 2rpx 15px 0 rgba(0, 0, 0, 0.34);
    border-radius: 20rpx;
    overflow: hidden;
}

.fixed_ticket_box .ticket_image {
    width: 630rpx;
    height: 252rpx;
    margin: 20rpx 20rpx 30rpx 20rpx;
    border-radius: 8rpx;
    vertical-align: top;
}

.fixed_ticket_box .line {
    width: 592rpx;
    height: 0;
    border-bottom: #e9ebec dashed 4rpx;
    margin-left: 40rpx;
}

.fixed_ticket_box .semicircle {
    width: 36rpx;
    height: 36rpx;
    border-radius: 50%;
    background: #4b4b4b;
    position: absolute;
    top: 287rpx;
}

.fixed_ticket_box .semicircle_left {
    left: -20rpx;
}

.fixed_ticket_box .semicircle_right {
    right: -20rpx;
}

.store_title {
    width: 100%;
    height: 40rpx;
    font-size: 40rpx;
    line-height: 40rpx;
    color: #262C32;
    text-align: center;
    margin-top: 30rpx;
    font-weight: bold;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.ticket_id {
    width: 100%;
    height: 40rpx;
    font-size: 28rpx;
    color: #919599;
    margin-top: 15rpx;
    text-align: center;
}

.qr_code {
    width: 300rpx;
    height: 300rpx;
    display: inline-block;
    margin: 35rpx 0 0 185rpx;
}

.code_tip {
    width: 406rpx;
    height: 80rpx;
    font-size: 28rpx;
    color: #000000;
    text-align: center;
    line-height: 40rpx;
    margin: 32rpx 0 0 132rpx;
}

.tips {
    position: relative;
    width: 220rpx;
    font-size: 24rpx;
    color: #FF8400;
    text-align: center;
    line-height: 28rpx;
    padding-left: 39rpx;
    padding-bottom: 30rpx;
    margin: 7rpx 0 0 208rpx;
}

.tips_icon {
    position: absolute;
    top: 0;
    left: 0;
    width: 26rpx;
    height: 26rpx;
}

.close {
    width: 60rpx;
    height: 60rpx;
    margin: 60rpx auto 0;
    position: relative;
    z-index: 1;
}

.close image {
    width: 60rpx;
    height: 60rpx;
}