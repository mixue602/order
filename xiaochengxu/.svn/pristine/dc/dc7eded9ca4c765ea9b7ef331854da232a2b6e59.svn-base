page{
  background: #f8f8f8;
}
page{
  background-color: #fff;
}
.container{
  background: #fff;
}
.cont{
  width: 590rpx;
  padding:0 80rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.logo{
  width: 300rpx;
  height: 300rpx;
  margin-top: 100rpx;
  margin-bottom: 80rpx;
}

.login-btn {
  background-image: linear-gradient(-90deg, #fa1e8c 0%, #fc1e56 100%);
  width: 590rpx;
  line-height: 88rpx;
  border-radius: 55rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
}

.coupon-prod{
  position: relative;
  background: #f8f8f8;
}
.coupon-list{
  padding-bottom:30rpx;
  position:relative;
}

.loot-all .cprice{
  color: #a5a5a5;
}
.loot-all .goget{
   background-image:url('http://css.gomein.net.cn/gmpro/1.0.0/minapp/img/expired@2x.png');
}
.loot-all .goget-txt{
  margin:42rpx 0 0 34rpx;
  color: #a5a5a5;
}

.coupon-info-no{
  margin: 20rpx auto;
  text-align: center;
}

.info-box{
  position: relative;
  width:690rpx;
  height: 240rpx;
  border:2rpx solid #ebebeb;
  background: #fff;
  margin-left: 30rpx;
  margin-top: 30rpx;
  box-sizing:border-box;
}

.coupon-parameter{
  width:556rpx;
  padding-left:30rpx;
  overflow: hidden;
}
.cprice{
  font-size:52rpx;
  padding-top:40rpx;
  color:#679BE5;
  line-height: 52rpx;
  display: flex;

}
.cprice .icon {
  font:30rpx/30rpx 'Arial';
  padding:15rpx 5rpx 0 0;
  float: left;
  font-weight: bold;
}
.cprice .cprice-tip{
  display:inline-block;
  float:left;
  font-weight: bold;
}
.cprice .coupon-type{
  font:24rpx/24rpx 'Microsoft YaHei';
  padding:20rpx 0 0 10rpx;
  display:inline-block;
  float: left;
}

.coupname{
  margin-top: 33rpx;
  font:400 24rpx/30rpx "Microsoft YaHei";
  color:#a5a5a5;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  flex:1;
  padding-right:30rpx;
}

.time {
  height:45rpx;
  overflow:hidden;
  padding-top:16rpx;
  color:#a5a5a5;
  font:24rpx/30rpx 'Microsoft Yahei';
}
.goget{
  height:240rpx;
  width:100rpx;
  position: absolute;
  top:-1rpx;
  right:0;
  background-image:url('http://css.gomein.net.cn/gmpro/1.0.0/minapp/img/blue@2x.png');
  background-size: 100% 100%;
 
}
.goget-txt{
  width:37rpx;
  height: 145rpx;
  display:block;
  font:24rpx/34rpx 'Microsoft Yahei';
  vertical-align: middle;
  color:#fff;
  margin:25rpx 0 0 34rpx;
  text-align: center;
}

.wetoast__bd__title{
  text-align: center;
}