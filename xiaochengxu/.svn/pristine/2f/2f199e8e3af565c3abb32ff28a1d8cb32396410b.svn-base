page{
	font-family: PingFangSC-Regular,Helvetica Neue,Helvetica,sans-serif;
	height: 100%;
}
.choose-coupon-container {
  overflow:hidden;
  height: 100%;
  background: #f5f5f5;
}
.loadingInt {
  background: rgba(0,0,0,0);
}

/* ==== tab ==== */
.weui-tab {
  position:relative;
  height:100%;
  overflow: hidden;
}
.weui-navbar {
  position:absolute;
  left: 0;
  top: 0;
  z-index: 10000;
  width: 100%;
  border-top: 1rpx solid #e6e6e6;
  border-bottom: 1rpx solid #e6e6e6;
  display:-webkit-flex;
  display:flex;
  height: 84rpx;
  line-height: 82rpx;
  box-sizing: border-box;
  color: #5A6066;
  background: #fff;
}
.weui-navbar__item {
  display:block;
  -webkit-flex:1;
  flex:1;
  text-align:center;
  font-size:0;
}
.weui-navbar__item.weui-bar__item_on {
  color:#E3101E;
}
.weui-navbar__title {
  display:inline-block;
  font-size:28rpx;  
}

.weui-navbar__slider {
  position:absolute;
  content:" ";
  left:0;
  bottom:-1rpx;
  width:240rpx;
  height:4rpx;
  background-color:#F20C59;
  -webkit-transition:left 0.3s;
  transition:left 0.3s;
  transition:left 0.3s;
  transition:left 0.3s,left 0.3s;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
}

/* ==== 优惠券说明 ==== */
.use-explain-skip-wrap {
  position:absolute;
  left: 0;
  top: 84rpx;
  z-index: 10000;
  width: 100%;
}
.use-explain-wrap {
  position: relative;
  background: #FFF7D2;
  padding: 16rpx 22rpx 16rpx 24rpx;
  font-size: 26rpx;
  line-height: 38rpx;
  min-height: 70rpx;
  box-sizing: border-box;
  color: #262C32;
}
.use-explain-wrap .coupon-tip {
  margin-right: 166rpx;
  box-sizing: border-box;
}
.use-explain-wrap .coupon-tip text {
  color: #E3101E;
  font-size: 28rpx;
}

.use-explain-wrap .use-explain {
  position: absolute;
  top: 16rpx;
  right: 22rpx;
  height: 100%;
  width: 130rpx;
  height: 38rpx;
  padding-right: 25rpx;
  box-sizing: border-box;
}

.use-explain-wrap .use-explain .arrow-right {
  position: absolute;
  right: 0;
  top: 8rpx;
  width: 15rpx;
  height: 27rpx;
} 


/* ==== 券列表 ==== */
scroll-view {
  box-sizing: border-box;
  padding-top: 168rpx;
  padding-bottom: 90rpx;
  color: #262C32;
}
.no-available-coupons-wrap {
  padding-top: 84rpx;
  padding-bottom: 0;
}
.choose-content-wrap {
  padding: 20rpx;
  
}
.choose-content-wrap .coupon-list-wrap {
  margin-bottom: 20rpx;
}
.choose-content-wrap .coupon-list-wrap:last-of-type {
  margin-bottom: 0;
}

.choose-content-wrap .coupon-box {
  height: 200rpx;
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  box-shadow: 0 4rpx 12rpx 8rpx rgba(0,0,0,0.04);
  border-radius: 12rpx;
  overflow: hidden;
}
.choose-content-wrap.available-coupons .coupon-box.no-available-coupon {
  opacity: 0.6;
}
.choose-content-wrap .price-box {
  height: 100%;
  -webkit-flex: 0 0 214rpx;
  flex: 0 0 214rpx;
  color: #fff;
  background-image: linear-gradient(-30deg, #34A7ED 0%, #55BEFD 100%);
  text-align: center;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-justify-content: center;
  justify-content: center;
  border-radius: 12rpx 0 0 12rpx;

}
.choose-content-wrap .price-box .price {
  font-size: 72rpx;
  line-height: 72rpx;
  font-weight: bold;
}
.choose-content-wrap .price-box .price text {

  font-size: 32rpx;
  vertical-align: middle;
}
.choose-content-wrap .price-box .price.small {
  font-size: 60rpx;
  line-height: 60rpx;
}
.choose-content-wrap .price-box .txt {
  display: block;
  font-size: 26rpx;
  line-height: 26rpx;
}

.choose-content-wrap .coupon-detail-box {
  -webkit-flex: auto;
  flex: auto;
  padding: 24rpx 100rpx 22rpx 24rpx;
  position: relative;
  border-radius: 0 12rpx 12rpx 0;
  height: 100%;
  background: #fff;
  box-sizing: border-box;
}
.not-available-coupons .coupon-detail-box {
  padding-right: 24rpx;
}
.choose-content-wrap .coupon-detail-box::before, .choose-content-wrap .coupon-detail-box::after {
  content: '';
  position: absolute;
  left: -12rpx;
  z-index: 2;
  width: 24rpx;
  height: 24rpx;
  background: #e9ebed;
  border-radius: 50%;
}
.choose-content-wrap .coupon-detail-box::before {
  top: -12rpx;
}
.choose-content-wrap .coupon-detail-box::after {
  bottom: -12rpx;
}
.choose-content-wrap .coupon-detail-box .detail-txt {
  font-size: 28rpx;
  line-height: 36rpx;
  color: #333;

  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;

}
.choose-content-wrap .coupon-detail-box .label {
  position: absolute;
  bottom: 62rpx;
  left: 24rpx;
  height: 30rpx;
  padding: 0 10rpx;
  background: #fff;
  border: 1rpx solid #2ba4ee;
  color: #2ba4ee;
  font-size: 22rpx;
  line-height: 28rpx;
  box-sizing: border-box;
  text-align: center;
  border-radius: 100rpx;
}
.choose-content-wrap .coupon-detail-box .validity {
  position: absolute;
  bottom: 22rpx;
  left: 24rpx;
  font-size: 22rpx;
  line-height: 22rpx;
  color: #b3b3b3;
}
/* 运费券 */
.choose-content-wrap .coupon-list-wrap.FEE .price-box {
  background-image: linear-gradient(-35deg, #FF9100 0%, #FFB300 100%);
}
.choose-content-wrap .coupon-list-wrap.FEE .coupon-detail-box .label {
  border: 1rpx solid #FA7A19;
  color: #FA7A19;
}
/* 蓝券 */
.choose-content-wrap .coupon-list-wrap.BLUE .price-box {
  background-image: linear-gradient(-30deg, #34A7ED 0%, #55BEFD 100%);
}
.choose-content-wrap .coupon-list-wrap.BLUE .coupon-detail-box .label {
  border: 1rpx solid #2BA4EE;
  color: #2BA4EE;
}
/* 红券 */
.choose-content-wrap .coupon-list-wrap.RED .price-box {
  background-image: linear-gradient(-221deg, #FA396D 0%, #EB265A 100%);
}
.choose-content-wrap .coupon-list-wrap.RED .coupon-detail-box .label {
  border: 1rpx solid #FF0044;
  color: #FF0044;
}
/* 美券 */
.choose-content-wrap .coupon-list-wrap.GOME .price-box {
  background-image: linear-gradient(-34deg, #D12EB6 0%, #F048D4 100%);
}
.choose-content-wrap .coupon-list-wrap.GOME .coupon-detail-box .label {
  border: 1rpx solid #D900B4;
  color: #D900B4;
}
/* 购物券 */
.choose-content-wrap .coupon-list-wrap.GOU_WU .price-box {
  background-image: linear-gradient(-34deg, #D12EB6 0%, #F048D4 100%);
}
.choose-content-wrap .coupon-list-wrap.GOU_WU .coupon-detail-box .label {
  border: 1rpx solid #D900B4;
  color: #D900B4;
}

/* 店铺券、购物券、商品券、 */
.choose-content-wrap .coupon-list-wrap.SHOP .price-box, .choose-content-wrap .coupon-list-wrap.GOU_WU .price-box, .choose-content-wrap .coupon-list-wrap.PRODUCT .price-box {
  background-image: linear-gradient(-31deg, #51AD2A 0%, #5ACC29 100%);
}
.choose-content-wrap .coupon-list-wrap.SHOP .coupon-detail-box .label, .choose-content-wrap .coupon-list-wrap.GOU_WU .coupon-detail-box .label, .choose-content-wrap .coupon-list-wrap.PRODUCT .coupon-detail-box .label {
  border: 1rpx solid #4AB61B;
  color: #4AB61B;
}

.choose-content-wrap .select-box {
  position: absolute;
  right: 32rpx;
  top: 50%;
  margin-top: -20rpx;
  width: 40rpx;
  height: 40rpx;
}
.choose-content-wrap .select-box image {
  width: 40rpx;
  height: 40rpx;
}

.choose-content-wrap .coupon-des {
  background: #ebeced;
  border: 1rpx solid #dcddde;
  border-top: 0;
  border-radius: 0 0 12rpx 12rpx;
  margin: 0 18rpx;
  padding: 20rpx 10rpx 20rpx 30rpx;
  font-size: 24rpx;
  color: #7A7F85;
  line-height: 30rpx;
}

.choose-coupon-container .confirm {
  position: absolute;
  left: 0;
  bottom: 0;
  background-image: linear-gradient(-222deg, #FC326C 0%, #F01851 100%);
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 34rpx;
  text-align: center;
  color: #fff;
}

/* ==== 暂无不可用优惠券 ==== */
.no-coupon {
  text-align: center;
  padding-top: 300rpx;
}
.no-coupon image {
  width: 160rpx;
  height: 135rpx;
}
.no-coupon .des {
  padding-top: 64rpx;
  font-size: 28rpx;
  color: #999;
  line-height: 28rpx;
}