/**商品详情页**/
@import "/lib/wxParse/wxParse.wxss";

page {
  background: #eee;
}

swiper {
  height: auto;
}

.container {
  position: relative;
}

.prodInfo {
  padding-bottom: 80rpx;
  display: block;
  position: relative;
}

/*轮播图*/

.solid-box {
  border-bottom: 2rpx solid #eee;
}

.solid-box  swiper {
  height: 750rpx;
}

.slide-image {
  width: 750rpx;
  height: 750rpx;
}

/*商品标题和价格*/

.prod-tit {
  width: 700rpx;
  padding: 10rpx 25rpx 20rpx 25rpx;
  background: #fff;
  margin-top: 2rpx;
}

.prod-tit-name {
  font-size: 28rpx;
  line-height: 1.5em;
  font-family: "Microsoft YaHei";
  color: #333;
  /*max-height: 100rpx;*/
  padding: 10rpx 0;
  overflow: hidden;
}

.prod-tit-name .tit-icon {
  float: left;
  display: inline-block;
  border: 1px solid #ef3030;
  padding: 0 5rpx;
  /* margin-top: 13rpx; */
  margin-right: 10rpx;
  font-size: 22rpx;
  line-height: 1.5em;
  border-radius: 5rpx;
  color: #ef3030;
  text-align: center;
}

/*促销介绍*/

.prod-desc {
  font: 500 24rpx/40rpx "Microsoft YaHei";
  color: #ef3030;
  max-height: 80rpx;
  overflow: hidden;
}

/*价格*/

.prod-price {
  width: 750rpx;
  color: #fff;
  position: relative;
  margin-top: -1rpx;
}

.prod-price>image {
  width: 100%;
  height: 126rpx;
  position: relative;
}

.price-box {
  position: absolute;
  width: 620rpx;
  height: 48rpx;
  padding: 39rpx 65rpx;
  color: #fff;
  top: 0;
  left: 0;
  overflow: hidden;
}

.price-box .price-num {
  font: 500 48rpx/48rpx   "Microsoft YaHei";
  float: left;
}

.price-box  .price-icon {
  float: left;
  font: 500 26rpx "Microsoft YaHei";
  color: #fff;
  padding-right: 5rpx;
  margin-top: 11rpx;
}

.price-box  navigator {
  float: right;
  font: 500 32rpx/48rpx "Microsoft YaHei";
}

.price-box  navigator  image {
  width: 18rpx;
  height: 32rpx;
  display: block;
  float: right;
  margin-top: 10rpx;
  margin-left: 10rpx;
}

/*会员价*/

.prod-viprice {
  font: 500 40rpx/50rpx "Microsoft YaHei";
  height: 50rpx;
  overflow: hidden;
  color: #e3101e;
  padding: 10rpx 0;
}

.prod-viprice .vip-grade {
  float: left;
  color: #333;
  font-size: 24rpx;
}

.prod-viprice .vip-grade text {
  display: inline-block;
  color: #ef3030;
}

.prod-viprice .price-icon {
  float: left;
  color: #ef3030;
  font-size: 18rpx;
}

/*促销语*/

.prod-sales, .parameter {
  width: 710rpx;
  overflow: hidden;
}

.prod-sales {
  height: 50rpx;
}

.tit-name {
  float: left;
  font: 500 28rpx/50rpx "Microsoft YaHei";
  color: #a2a4a6;
}

.sales {
  float: left;
  width: 600rpx;
  padding: 0 10rpx;
}

.sales-info, .parameter-box, .address-info {
  float: left;
  width: 600rpx;
  font: 500 28rpx/50rpx "Microsoft YaHei";
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.sales-spread {
  height: auto;
  white-space: normal;
}

.sales-icon {
  display: inline-block;
  font-size: 24rpx;
  line-height: 44rpx;
  border: 1rpx solid #ef3030;
  padding: 0 5rpx;
  border-radius: 10rpx;
  color: #ef3030;
  margin-right: 10rpx;
}

.red {
  color: #ef3030;
  font-weight: 600;
}

.parameter-icon {
  float: right;
  padding-top: 8rpx;
}

.parameter-icon image {
  width: 30rpx;
  height: 30rpx;
}

/*已选*/

.prod-properties {
  width: 710rpx;
  padding: 20rpx;
  background: #fff;
  margin-top: 20rpx;
}

.parameter-box {
  white-space: normal;
}

.parameter-box text {
  display: inline-block;
  padding: 0 10rpx;
  overflow: hidden;
}

.arrowdown {
  width: 30rpx;
  height: 30rpx;
  transform: rotate(90deg);
}

/*参数*/

.prod-property {
  display: flex;
  overflow: hidden;
  margin-top: 10rpx;
}

.prod-property-left {
  flex: 1;
}

.prod-property-right {
  flex: 5;
}

.prod-properties.up {
  height: 50rpx;
  overflow: hidden;
}

.prod-property  .tit-name {
  margin-top: 20rpx;
  text-align: right;
  float: none;
}

.property-cont {
  font: 500 28rpx/50rpx "Microsoft YaHei";
  overflow: hidden;
}

.property-btn {
  float: left;
  margin: 10rpx 0 10rpx 20rpx;
  padding: 5rpx 30rpx;
  background-color: #fff;
  border: 2rpx solid #ddd;
  border-radius: 5rpx;
  color: #e6e6e6;
}

.property-btn.set {
  border: 2rpx solid #ddd;
  color: #333;
}

.property-btn.no {
  border: 2rpx solid #ddd;
  color: #ddd;
}

.curr {
  color: #e3101e;
  border: 2rpx solid #e3101e;
}

.gray {
  background-color: #eee;
  color: #a2a4a6;
}

/*数量*/

.prod-num {
  padding: 10rpx 0;
  overflow: hidden;
  display: flex;
}

.prod-num .tit-name {
  line-height: 60rpx;
  flex: 1;
  text-align: right;
}

.property-num-box {
  flex: 5;
}

.property-num {
  /* float: left; */
  border: 1rpx solid #eee;
  border-radius: 5rpx;
  font: 500 28rpx "Microsoft YaHei";
  overflow: hidden;
  margin-left: 20rpx;
  display: inline-block;
}

.property-num view {
  float: left;
  width: 45rpx;
  text-align: center;
  padding: 5rpx 0;
  font: 600 32rpx/50rpx "Microsoft YaHei";
  background: #eee;
}

.property-num input {
  float: left;
  width: 98rpx;
  height: 50rpx;
  min-height: 50rpx;
  padding: 5rpx 0;
  text-align: center;
  border-left: 1rpx #eee solid;
  border-right: 1rpx #eee solid;
  font: 400 34rpx/50rpx "Microsoft YaHei";
}

.num-limit {
  float: left;
  padding-left: 10rpx;
  height: 48rpx;
  font: 500 28rpx/48rpx "Microsoft YaHei";
  padding: 5rpx 0;
  color: #666;
  padding-left: 30rpx;
}

/*地址*/

.prod-address {
  background: #fff;
  width: 710rpx;
  padding: 20rpx;
  margin-top: 20rpx;
}

.address-info {
  width: 600rpx;
  height: 50rpx;
  overflow: hidden;
}

.address-info text {
  padding: 0 10rpx;
}

/*是否有货*/

.prod-available {
  width: 656rpx;
  font: 400 24rpx/40rpx "Microsoft YaHei";
  overflow: hidden;
}

.prod-available  .wxParse {
  margin: 0;
  font-family: "Microsoft YaHei";
  font-size: 24rpx;
  color: #a2a4a6;
  line-height: 40rpx;
}

.ava-tit {
  float: left;
  color: #ef3030;
}

.ava-cont {
  width: 600rpx;
  padding: 0 10rpx;
  color: #a2a4a6;
  display: inline;
}

.ava-cont .arrival_time {
  display: inline;
  font-weight: 600;
}

.ava-cont text {
  display: inline-block;
  padding: 0 10rpx;
}

/*标签*/

.prod-tags {
  width: 666rpx;
  padding: 10rpx 20rpx 10rpx 64rpx;
  font: 400 24rpx/40rpx "Microsoft YaHei";
  overflow: hidden;
}

.prod-tags icon {
  vertical-align: -10%;
}

.prod-tags view {
  float: left;
  padding-right: 20rpx;
  color: #a2a4a6;
}

.tags-circle {
  display: inline-block;
  width: 12px;
  height: 12px;
}

/*评价*/

.prod-appraise {
  width: 710rpx;
  padding: 20rpx;
  margin-top: 20rpx;
  background: #fff;
  overflow: hidden;
}

.appraise-tit {
  width: 710rpx;
  height: 50rpx;
  overflow: hidden;
  font: 500 26rpx/50rpx "Microsoft YaHei";
}

.appraise-tit .tit-name {
  color: #333;
  width: 300rpx;
}

.appraise-tit .tit-name text {
  display: inline-block;
  padding: 0 10rpx;
}

.tit-cont {
  float: right;
}

.tit-cont text {
  display: inline-block;
  padding: 0 10rpx;
  color: #ef3030;
}

/*评价内容*/

.appraise-info {
  width: 710rpx;
  padding: 10rpx 0;
}

.user-info {
  font-size: 24rpx;
  height: 32rpx;
  padding: 10rpx 0;
  overflow: hidden;
}

.user-name {
  float: left;
  color: #333;
  height: 32rpx;
  line-height: 32rpx;
  padding-right: 10rpx;
}

.user-grade {
  float: left;
  padding: 2rpx;
  border: 1rpx solid #ef3030;
  color: #ef3030;
  font-weight: 500;
  border-radius: 5rpx;
  line-height: 26rpx;
}

/*五星*/

.user-start {
  position: relative;
  float: left;
  margin-left: 20rpx;
  width: 60px;
  height: 12px;
  background: url("http://css.gomein.net.cn/gmpro/1.0.0/minapp/img/startbg.png") repeat-x;
}

.user-start-yellow {
  position: absolute;
  left: 0;
  top: 0;
  height: 12px;
  background: url("http://css.gomein.net.cn/gmpro/1.0.0/minapp/img/start.png") repeat-x;
}

.appraise-info .appraise-text {
  font-size: 26rpx;
  line-height: 30rpx;
  padding: 15rpx 0;
  color: #333;
}

.appraise-parameter {
  color: #a2a4a6;
  line-height: 35rpx;
}

.appraise-parameter >view {
  font-size: 24rpx;
}

/*商品详情导航栏*/

.prod-items-info .tips {
  display: block;
  padding: 20rpx;
  text-align: center;
  font: 400 30rpx "sans-serif";
}

.tips text {
  padding-left: 20rpx;
}

.prod-info {
  width: 750rpx;
  margin-top: 20rpx;
  background: #fff;
}

.prod-info .prod-nav {
  width: 100%;
  border-top: 1rpx solid #e6e6e6;
  border-bottom: 1rpx solid #e6e6e6;
}

.prod-nav .nav-items {
  float: left;
  width: 374rpx;
  height: 50rpx;
  padding: 20rpx 0;
  font: 400 25rpx/50rpx "Microsoft YaHei";
  text-align: center;
}

.on {
  color: #ef3030;
  padding-bottom: 15rpx;
  border-bottom: 5rpx solid #ef3030;
}

.prod-nav .nav-icon {
  float: left;
  height: 50rpx;
  margin-top: 20rpx;
  width: 2rpx;
  background: #e6e6e6;
}

.prod-items-info >view {
  display: none;
  min-height: 300rpx;
}

.prod-items-info .prod-spec {
  text-align: left;
  margin: 10rpx 20rpx 40rpx 20rpx;
  width: 710rpx;
  font: 400 25rpx/60rpx "Microsoft YaHei";
  border-top: 1rpx solid #c1c4c5;
}

.prod-items-info .prod-spec .spec-cont {
  display: block;
  font-weight: 600;
}

.prod-items-info .prod-spec >view {
  border-right: 1rpx solid #c1c4c5;
  border-left: 1rpx solid #c1c4c5;
  border-bottom: 1rpx solid #c1c4c5;
  text-align: left;
}

.prod-items-info .prod-spec>view .spec-tit {
  font-weight: 600;
  display: inline-block;
  padding-left: 20rpx;
  width: 278rpx;
  border-right: 1rpx solid #c1c4c5;
}

.prod-items-info .prod-spec>view .spec-text {
  padding-left: 20rpx;
}

.prod-items-info .prod-spec .spec-cont text {
  width: 688rpx;
  padding-left: 20rpx;
}

.prod-ped, .prod-after {
  font: 400 25rpx/50rpx "Microsoft YaHei";
  padding: 20rpx;
}

.prod-items-info .active {
  display: block;
}

/*立即购买*/

.buyBox {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  border: 1rpx solid #e6e6e6;
  background: #fff;
  z-index: 99999999;
  display: none;
}
.show{
  display: block;
}
.buyBtn {
  width: 296rpx;
  height: 98rpx;
  background-color: #ff5757;
  border: 1rpx solid #ff5757;
  color: #fff;
  text-align: center;
  font: 500 32rpx  "Microsoft YaHei";
  float: right;
  z-index: 99999999;
  line-height: 98rpx;
}
.notStarted{
  border-color: #57aa59;
  background: #57aa59;
}

.buyBtnHover {
  background-color: #e31013;
  border: 1rpx solid #e31013;
}

.buyBtnNo {
  width: 296rpx;
  height: 98rpx;
  background-color: #ddd;
  color: #fff;
  border: 1rpx solid  #ddd;
  text-align: center;
  font: 500 32rpx  "Microsoft YaHei";
  margin: 0 auto;
  float: right;
  z-index: 99999999;
  line-height: 98rpx;
  border-bottom: none;
  border-top: none;
}

.buyBtnNoHover {
  background-color: #ff8024;
  border: 1rpx solid #ff8024;
}

/*状态*/

.buyStatus {
  width: 446rpx;
  height: 98rpx;
  background-color: #ff5757;
  border: 1rpx solid #ff5757;
  text-align: center;
  color: #fff;
  font: 500 26rpx "Microsoft YaHei";
  display: inline-block;
  position: absolute;
  left: 0;
  z-index: 99999999;
  line-height: 98rpx;
}

.buyEnd{
   background-color: #fff;
    border: 1rpx solid #e6e6e6;
  color: #5e5e5e;
   border-bottom: none;
  border-top:none;
}
