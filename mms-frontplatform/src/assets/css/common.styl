}

.el-message.el-message--info {
    top: 70px;
}

.el-main .el-input__inner,
.el-main .el-input__inner:focus,
.el-main .textarea-box,
.el-main .textarea-box:focus{ color:#333;}
.el-main .el-form-item.is-success .el-input__inner,
.el-main .el-form-item.is-success .el-input__inner:focus,
.el-main .el-form-item.is-success .el-textarea__inner,
.el-main .el-form-item.is-success .el-textarea__inner:focus{ border-color: #ccc; color:#333;}

.el-main .el-form-item.is-required .el-form-item__label:before{ color:#ff0027;}
.el-main .red{ color:#ff0027!important;}

.el-message.el-message--info,
.el-message.el-message--warning,
.el-message.el-message--success,
.el-message.el-message--error{ background-color: #fff;}

.el-message.el-message--warning .el-message__content,
.el-message.el-message--success .el-message__content,
.el-message.el-message--error .el-message__content{ color:#909399;}

.el-message.el-message--info .el-icon-info{ font-size:20px; color: #4990e2}
.el-message.el-message--warning .el-icon-warning{ font-size:20px; color: #ff9549;}
.el-message.el-message--success .el-icon-success{ font-size:20px; color: #6ebf50;}
.el-message.el-message--error .el-icon-error{ font-size:20px; color: #ff0027;}

.el-main .el-form-item__label{ color: #333;}

.el-message{ border:1px solid #ccc; box-shadow: 0 2px 10px rgba(50,50,50,.5)}


.el-select-dropdown__wrap.el-scrollbar__wrap
    scrollbar-3dlight-color:#fff; // 3d亮色阴影边框(threedlightshadow)的外观颜色
    scrollbar-highlight-color:#fff; // 滚动条3d高亮边框(threedhighlight)的外观颜色
    scrollbar-arrow-color:#fff; // 滚动条方向箭头的颜色
    scrollbar-shadow-color:#fff; // 滚动条方向箭头的颜色
    scrollbar-darkshadow-color:#fff; // 滚动条3d暗色阴影边框(threeddarkshadow)的外观颜色
    scrollbar-base-color:#fff; // 滚动条基准颜色
    scrollbar-track-color:#fff; // 滚动条拖动区域的外观颜色

/*定义滚动条轨道 内阴影+圆角*/  
.el-scrollbar__bar::-webkit-scrollbar-track  
    -webkit-box-shadow: inset 0 0 0 rgba(255,255,255,0)
    border-radius: 0
    background-color: rgba(255,255,255,0) 
  
/*定义滑块 内阴影+圆角*/  
.el-scrollbar__bar::-webkit-scrollbar-thumb  
    border-radius: 0
    -webkit-box-shadow: inset 0 0 0 rgba(255,255,255,0)
    background-color: rgba(255,255,255,0)
.el-scrollbar__bar::-webkit-input-placeholder
    color: #d3d3d3
.el-scrollbar__bar::-webkit-scrollbar
.el-select-dropdown__wrap.el-scrollbar__wrap::-webkit-scrollbar
    width: 0
    height: auto
    background-color: rgba(255,255,255,0)
