/* 图表上数据样式 */

.cdata-list {
    padding: 20px 0;
    border: 1px solid #ccc;
}

.cdata-tit {
    margin-top: 20px;
    margin-bottom: 10px;
    font-size: 14px;
    color: #000;
}

.cdata-table .cdata-item {
    position: relative;
    text-align: center;
}

.cdata-item:not(:last-child):after {
    content: "";
    position: absolute;
    top: 50%;
    right: 0;
    margin-top: -20px;
    width: 1px;
    height: 40px;
    background: #ddd;
}

.cdata-item .c-count {
    display: block;
    color: #555;
}

.cdata-item .c-num {
    display: block;
    margin-top: 12px;
    font-size: 16px;
    color: #333;
}

.el-main .c-more,
.el-main .c-more.el-button {
    display: inline-block;
    padding: 0;
    margin-left: 5px;
    width: 15px;
    height: 15px;
    color: #a5a5a5;
    border: 1px solid #a5a5a5;
    border-radius: 50%;
    text-decoration: none;
    text-align: center;
    cursor: pointer;
}

.cdata-item .c-more:hover {
    color: #73a3d9;
    border-color: #73a3d9;
}

.mm-btn {
    display: inline-block;
    cursor: pointer;
}


/* 图表区域样式 */

.chart-wrapper {
    width: 100%;
    overflow: hidden;
}

.chart-wrapper li {
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    margin-top: 20px;
    width: 50%;
    height: 500px;
}

.chart-wrapper li.pr10 {
    float: left;
    padding-right: 10px;
}

.chart-wrapper li.pl10 {
    float: left;
    padding-left: 10px;
}

.chart-area {
    width: 100%;
    height: 100%;
    border: 1px solid #ccc;
    overflow: hidden;
}

.chart-wrapper .el-col {
    margin-top: 20px;
    overflow: hidden;
}


/* elementUI 样式覆盖 */
.widget-main .el-menu--horizontal .el-menu-item {
    height: 26px;
    line-height: normal;
    padding: 0 10px;
    margin-right: 10px;
}

.widget-main .el-input--suffix el-input--suffix {
    background: #fff;
}

.widget-main .el-form-item {
    margin-bottom: 15px;
}

.widget-main .el-form-item__label,
.widget-main .el-form-item__content {
    line-height: 30px;
}

.widget-main .el-form-item__error {
    color: #ff0027;
}

.el-form-item.is-error .el-input__inner,
.el-form-item.is-error .el-input__inner:focus,
.el-form-item.is-error .el-textarea__inner,
.el-form-item.is-error .el-textarea__inner:focus,
.el-message-box__input input.invalid,
.el-message-box__input input.invalid:focus {
    border-color: #ff0027;
}

.widget-main .el-input__inner {
    height: 30px;
}

.widget-main .el-input--suffix .el-input__inner,
.widget-main .el-select>.el-input {
    width: 100%;
}

.el-select input[readonly][type=text] {
    text-align: left;
}

.el-main .el-select__input {
    background-color: transparent!important;
    border: none;
}

.mm-date-nolabel .el-form-item__content {
    margin-left: 10px!important;
}
.lr-wrapper .el-input__icon{ line-height:32px;}
.el-message-box__wrapper .el-message-box{ border:none;}
.el-message-box .el-message-box__header{ background:#4990e2;}
.el-message-box .el-message-box__title{ color:#fff; font-size:16px;}
.el-message-box .el-message-box__headerbtn .el-message-box__close{ color:#fff;}
.el-message-box .el-message-box__btns{ text-align:center;}
.el-message-box .el-button--default{ border:1px solid #409EFF;}


/* messagebox warning改成蓝色问题图标 */
.el-main .el-message-box__status.el-icon-warning {
    color: #3a8ee6;
}

.el-main .el-message-box__status.el-icon-warning:before {
    content: "\E634";
}

.el-main .el-radio {
    line-height: 30px;
}

.el-main .el-button {
